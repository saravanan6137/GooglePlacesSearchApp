{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["src/components/*"],
      "@screens/*":  ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@storage/*": ["src/storage/*"],
      "@constants/*": ["src/constants/*"],
      "@assets/*": ["src/assets/*"],
      "@types/*": ["src/types/*"],
    },
  }
}
